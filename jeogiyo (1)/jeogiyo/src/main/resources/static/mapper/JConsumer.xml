<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace='com.jpro.jmybatis.JConsumerMapper'>

	<select id="totSize" parameterType="Page" resultType="Integer">
		select count(mainLocal) from jstoreVo
		where storeName like "%${findStr}%"
		or    address  like '%${findStr}%'
	</select>
	
	<select id="storeSelect" parameterType="Page" resultType="JConsumerVo">
		select count(mainLocal) from jstoreVo
		where storeName like "%${findStr}%"
		or    address  like '%${findStr}%'
		limit ${startNo-1}, ${listSize}
	</select>
	
